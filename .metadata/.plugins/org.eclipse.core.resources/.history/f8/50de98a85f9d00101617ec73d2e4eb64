		System.out.println("Teclee el primer número:");
		num1 = teclado.nextInt();
		System.out.println("Teclee el segundo número:");
		num2 = teclado.nextInt();

		if (num1 > num2) {
			mayor = num1;
		} else {
			mayor = num2;
		}

		while (numero < mayor) {
			
		}

	}

}
