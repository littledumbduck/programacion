package boletin_4;

import java.util.Scanner;

public class ejercicio15 {

	public static void main(String[] args) {

		double num = 1;
		double division;
		Scanner teclado = new Scanner(System.in);

		while (num <= 100) {
			division = num / 2;
			if (division % 1 == 0) {
				System.out.println(division);
			}
			
			num++;

		}

	}
}
