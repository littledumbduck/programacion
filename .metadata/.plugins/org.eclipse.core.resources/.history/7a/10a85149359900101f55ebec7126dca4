package boletin_2;

import java.util.*;

public class ejercicio1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Teclee el número de años");
		int anos;
		Scanner teclado = new Scanner(System.in);
		int horas = 0;
		anos = teclado.nextInt();
		horas = 24 * 365 * anos;
		System.out.println("En " + anos + " años hay " + horas + " horas.");
		teclado.close();
	}

}
